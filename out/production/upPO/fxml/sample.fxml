<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="200.0" prefWidth="250.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sourse.Controller">
   <children>
      <VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="100.0" prefWidth="250.0">
         <children>
            <Label alignment="CENTER" prefHeight="17.0" prefWidth="150.0" text="Выберите комплект">
               <VBox.margin>
                  <Insets bottom="10.0" left="50.0" top="40.0" />
               </VBox.margin>
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <ComboBox fx:id="ComboboxComplect" prefWidth="150.0">
               <VBox.margin>
                  <Insets left="50.0" />
               </VBox.margin>
               <items>
                  <FXCollections fx:factory="observableArrayList">
                     <String fx:value="Control MX" />
                     <String fx:value="Starline" />
                  </FXCollections>
               </items>
            </ComboBox>
         </children>
      </VBox>
      <HBox minWidth="-Infinity" prefHeight="100.0" prefWidth="200.0">
